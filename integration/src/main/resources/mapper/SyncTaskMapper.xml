<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.udan.ubsp.integration.mapper.SyncTaskMapper">

    <!-- 根据任务编码查询任务 -->
    <select id="selectByTaskCode" resultType="com.udan.ubsp.integration.entity.SyncTaskEntity">
        SELECT * FROM ubsp_di_sync_task 
        WHERE task_code = #{taskCode}
    </select>

</mapper>